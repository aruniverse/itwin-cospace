{
  "name": "itwin-devspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "lage build",
    "build:clean": "lage build --no-cache",
    "build:clean:profile": "lage build --no-cache --profile",
    "clean": "lage clean && rimraf node_modules",
    "build:turbo": "turbo run build",
    "clean:turbo": "turbo run clean && rimraf node_modules",
    "list-pkgs": "pnpm ls -r --depth -1"
  },
  "devDependencies": {
    "lage": "latest",
    "rimraf": "^3.0.2",
    "turbo": "latest"
  },
  "packageManager": "pnpm@6.29.1",
  "//note": "run `pnpm ls -r --depth -1` to see all pkgs in the workspace, then for each one set the override to `workspace:*`",
  "pnpm": {
    "overrides": {
      "@itwin/core-backend": "workspace:*",
      "@itwin/core-bentley": "workspace:*",
      "@itwin/core-common": "workspace:*",
