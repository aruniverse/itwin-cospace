{
  "name": "itwin-cospace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "lage build",
    "build:clean": "lage build --no-cache",
    "build:profile": "lage build --no-cache --profile",
    "clean": "lage clean --no-cache && cospace purge && pnpm i -w cospace",
    "setOverrides": "cospace override",
    "start:web": "pnpm start --filter @itwin/web-viewer-test",
    "start:desktop": "pnpm start --filter @itwin/desktop-viewer-test",
    "start:ts": "pnpm start --filter @itwin/typescript-viewer-sample",
    "start:abg": "pnpm start --filter @aruniverse/viewer"
  },
  "packageManager": "pnpm@6.32.3",
  "devDependencies": {
    "cospace": "^0.4.3",
    "lage": "1.5.1"
  },
  "pnpm": {
    "overrides": {
      "@bentley/react-scripts": "workspace:*",
      "@itwin/appui-abstract": "workspace:*",
      "@itwin/appui-layout-react": "workspace:*",
      "@itwin/appui-react": "workspace:*",
      "@itwin/backend-webpack-tools": "workspace:*",
      "@itwin/build-tools": "workspace:*",
      "@itwin/certa": "workspace:*",
      "@itwin/components-react": "workspace:*",
      "@itwin/core-backend": "workspace:*",
      "@itwin/core-bentley": "workspace:*",
      "@itwin/core-common": "workspace:*",
      "@itwin/core-electron": "workspace:*",
      "@itwin/core-extension": "workspace:*",
      "@itwin/core-frontend": "workspace:*",
      "@itwin/core-geometry": "workspace:*",
      "@itwin/core-i18n": "workspace:*",
      "@itwin/core-markup": "workspace:*",
      "@itwin/core-mobile": "workspace:*",
      "@itwin/core-orbitgt": "workspace:*",
      "@itwin/core-quantity": "workspace:*",
      "@itwin/core-react": "workspace:*",
      "@itwin/core-telemetry": "workspace:*",
      "@itwin/core-transformer": "workspace:*",
      "@itwin/core-webpack-tools": "workspace:*",
      "@itwin/desktop-viewer-react": "workspace:*",
      "@itwin/ecschema-editing": "workspace:*",
      "@itwin/ecschema-locaters": "workspace:*",
      "@itwin/ecschema-metadata": "workspace:*",
      "@itwin/ecschema2ts": "workspace:*",
      "@itwin/error-handling-react": "workspace:*",
      "@itwin/eslint-plugin": "workspace:*",
      "@itwin/express-server": "workspace:*",
      "@itwin/frontend-devtools": "workspace:*",
      "@itwin/geo-tools-react": "workspace:*",
      "@itwin/hypermodeling-frontend": "workspace:*",
      "@itwin/imodel-components-react": "workspace:*",
      "@itwin/imodel-react-hooks": "workspace:*",
      "@itwin/measure-tools-react": "workspace:*",
      "@itwin/perf-tools": "workspace:*",
      "@itwin/presentation-backend": "workspace:*",
      "@itwin/presentation-common": "workspace:*",
      "@itwin/presentation-components": "workspace:*",
      "@itwin/presentation-frontend": "workspace:*",
      "@itwin/presentation-testing": "workspace:*",
      "@itwin/property-grid-react": "workspace:*",
      "@itwin/tree-widget-react": "workspace:*",
      "@itwin/viewer-react": "workspace:*",
      "@itwin/web-viewer-react": "workspace:*",
      "@itwin/webgl-compatibility": "workspace:*",
      "internal-tools": "workspace:*"
    },
    "prefer-workspace-packages": true,
    "link-workspace-packages": "deep"
  }
}
