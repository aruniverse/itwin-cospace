{
  "name": "itwin-cospace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "repo-init": "zx ./scripts/init.mjs",
    "build": "lage build",
    "build:clean": "lage build --no-cache",
    "build:profile": "lage build --no-cache --profile",
    "build:turbo": "turbo run build",
    "clean": "lage clean --no-cache && cospace purge && pnpm i -w cospace",
    "setOverrides": "cospace override",
    "start:test": "pnpm start --filter @aruniverse/viewer"
  },
  "packageManager": "pnpm@6.32.3",
  "devDependencies": {
    "cospace": "^0.4.5",
    "lage": "1.6.2",
    "turbo": "^1.2.16",
    "zx": "7.0.0"
  }
}
